{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "ContainerApp.Acmebot": "Warning"
    }
  },
  "extensions": {
    "durableTask": {
      "hubName": "DurableFunctionsHub",
      "extendedSessionsEnabled": true,
      "extendedSessionIdleTimeoutInSeconds": 120
    },
    "http": {
      "routePrefix": "",
      "customHeaders": {
        "Cache-Control": "no-store"
      }
    }
  }
}